%footer.usa-footer.usa-footer-medium{:role => "contentinfo"}
  -#.usa-grid.usa-footer-return-to-top
  -#  %a{:href => "#"} Return to top
  = render partial: 'page/footer/footer_nav'
  .usa-footer-secondary_section
    .usa-grid
      .usa-footer-logo.usa-width-one-half
        = image_tag('wds18f/logo-img.png', alt: 'Logo image', class: 'usa-footer-circle-124')
        %h3.usa-footer-logo-heading Name of Agency
      .usa-footer-contact-links.usa-width-one-half
        %a.usa-link-facebook{:href => "javascript:void(0);"}
          %span Facebook
        %a.usa-link-twitter{:href => "javascript:void(0);"}
          %span Twitter
        %a.usa-link-youtube{:href => "javascript:void(0);"}
          %span YouTube
        %a.usa-link-rss{:href => "javascript:void(0);"}
          %span RSS
        %address
          %h3.usa-footer-contact-heading Agency Contact Center
          %p (800) CALL-GOVT
          %a{:href => "mailto:info@agency.gov"} info@agency.gov
/ Update the link path to where your JavaScript file is located. For example: /path/to/your/assets/js/lib/uswds.min.js
-#%script{:src => "/assets/js/vendor/uswds.min.js"}